<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    
        <record model="ir.module.category" id="module_backend">
            <field name="name">Auto Car Users</field>
            <field name="description">Helps you handle your backend security.</field>
            <field name="sequence">10</field>
        </record>

        <record id="group_vendor" model="res.groups">
            <field name="name">Auto Car Vendor</field>
            <field name="category_id" ref="module_backend" />
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]" />
        </record>
        
        <record id="group_admin" model="res.groups">
            <field name="name">Auto Car Admin</field>
            <field name="category_id" ref="module_backend" />
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>
